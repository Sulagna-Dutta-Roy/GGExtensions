{
    "manifest_version": 3,
    "name": "Tab Organizer",
    "version": "1.0",
    "description": "Organize your tabs into groups.",
    "permissions": [
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  