{
  "manifest_version": 3,
  "name": "ToDo Extension JS",
  "version": "1.0",
  "description": "Simple ToDo Extension is JS for managing day to day tasks...",
  "permissions": ["storage", "notifications", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "128": "logo.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
