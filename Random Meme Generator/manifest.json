{
  "manifest_version": 2,
  "name": "Random Meme Generator",
  "version": "1.0",
  "description": "A random meme generator and shareable extension.",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}