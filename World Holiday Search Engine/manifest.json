{
  "manifest_version": 3,
  "name": "World Holiday  Extension",
  "version": "1.0",
  "description": "Displays the holidays around the world.",
  "permissions": ["activeTab", "storage"],
  "action": {
      "default_popup": "index.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "index.html"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}