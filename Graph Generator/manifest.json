{
    "manifest_version": 3,
    "name": "3D Graph Generator",
    "version": "1.0",
    "description": "Generate interactive 3D graphs based on data and time.",
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
    }
  }
  