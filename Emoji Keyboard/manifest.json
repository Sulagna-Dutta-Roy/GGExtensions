{
    "name": "Emojiboard: Emoji Keyboard",
    "version": "1.0.0",
    "description": "Quick emoji selection",
    "permissions": ["clipboardWrite", "storage"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Emojiboard",
        "default_icon": {
            "16": "image16.png",
            "32": "image32.png",
            "48": "image48.png",
            "128": "image128.png"
        }
    },
    "icons": {
        "16": "image16.png",
        "32": "image32.png",
        "48": "image48.png",
        "128": "image128.png"
    },
    "manifest_version": 2
}
