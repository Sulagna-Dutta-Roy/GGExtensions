{
  "manifest_version": 3,
  "name": "SEO Analyzer",
  "version": "1.0",
  "description": "A tool to analyze the SEO of a webpage.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
