{
    "manifest_version": 3,
    "name": "Tic-Tac-Toe Game Extension JS",
    "version": "1.0",
    "description": "Multiplayer Tic-Tac-Toe game extension built in pure HTML CSS JS...",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "index.html"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}
