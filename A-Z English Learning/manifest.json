{
    "manifest_version": 3,
    "name": "A-Z Learning",
    "version": "1.0",
    "description": "An interactive tool to learn the alphabet from A to Z.",
    "icons": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icons/icon48.png"
      }
    },
    "permissions": [],
    "content_security_policy": {
      "extension_pages": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
    }
  }
  