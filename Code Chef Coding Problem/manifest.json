{
    "manifest_version": 3,
    "name": "CodeChef Coding Problem Navigator",
    "description": "Navigate to CodeChef coding problems",
    "version": "1.0",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  