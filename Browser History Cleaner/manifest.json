{
  "manifest_version": 3,
  "name": "Browser History Cleaner",
  "version": "1.0",
  "description": "Clear browsing history, cache, and cookies with a single click.",
  "permissions": [
    "history",
    "tabs",
    "browsingData"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}