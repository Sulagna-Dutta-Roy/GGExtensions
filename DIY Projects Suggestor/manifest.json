{
    "manifest_version": 3,
    "name": "DIY Projects Suggestor",
    "version": "1.0.0",
    "description": "Suggests DIY projects based on user input keywords.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["activeTab", "storage"]
  }
  