{
    "manifest_version": 3,
    "name": "Website Bandwidth Calculator",
    "version": "1.0",
    "description": "Calculate the bandwidth needed for your website based on page views, average page size, and page views per visitor.",
    "permissions": [],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }