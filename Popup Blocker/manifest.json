{
    "manifest_version": 3,
    "name": "Best Popup Blocker",
    "version": "1.0",
    "description": "A popup blocker extension with modern UI and animations",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "notifications",
      "storage",
      "https://*/*",
      "http://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  