{
    "manifest_version": 3,
    "name": "Social Media Quick Access",
    "version": "1.0",
    "description": "Quick access to your favorite social media platforms.",
    "permissions": ["activeTab", "notifications", "storage", "alarms"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "128": "icons/icon128.png"
    }
  }
  