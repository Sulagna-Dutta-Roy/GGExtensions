{
    "manifest_version": 3,
    "name": "Perl Code Editor Access",
    "version": "1.0",
    "description": "Navigate to Perl Code Editor functionality.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  