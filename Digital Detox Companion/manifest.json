{
    "manifest_version": 3,
    "name": "Digital Detox Companion",
    "version": "1.0",
    "description": "Helps you manage your online time and reduce digital distractions.",
    "permissions": ["tabs", "storage", "notifications", "alarms"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
      }
    }
    
  }
  
