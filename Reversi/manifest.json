{
    "manifest_version": 3,
    "name": "Reversi Game",
    "description": "Play Reversi (Othello) in your browser",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["activeTab"]
  }
  