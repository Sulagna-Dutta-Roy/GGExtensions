{
    "manifest_version": 3,
    "name": "Favicon Customizer",
    "version": "1.0",
    "description": "Customize the favicon of any website.",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  