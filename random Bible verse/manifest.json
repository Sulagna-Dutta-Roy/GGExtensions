{
  "manifest_version": 3,
  "name": "Random Bible Verse Extension",
  "version": "1.0",
  "description": "Displays a random Bible verse.",
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "icon16.png",
          "48": "icon48.png",
          "128": "icon128.png"
      }
  },
  "permissions": [
      "activeTab"
  ]
}
